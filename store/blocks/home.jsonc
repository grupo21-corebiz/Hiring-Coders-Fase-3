{
  "store.home": { 
    "blocks": [
      "flex-layout.row#text",
      "image#about-corebiz"
    ]
  },
  "flex-layout.row#text": {
    "children": [
      "flex-layout.col#texts"
    ]
  },
  "flex-layout.col#texts": {
    "children": [
      "rich-text#title",
      "rich-text#paragraph"
    ],
    "props": {
      "blockClass": "acorebiz-text"
    }
  },
  "rich-text#title": { 
    "props": { 
      "blockClass": "acorebiz-title",
      "text": "**a corebiz**",
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
      "font": "t-heading-1"
    }
  },
  "rich-text#paragraph": {
    "props": {
      "blockClass": "acorebiz-paragraph",
      "text": "conheça um pouco de nossa história, nosso crescimento e nossa cultura.",
      "font": "small"
    }
  },
  "image#about-corebiz": {
    "props": {
      "src": "https://www.corebiz.ag/wp-content/uploads/2020/06/about-intro-image.jpg",
      "blockClass": "intro-about-corebiz",
      "fullWidth": true,
      "width": "grow"
    }
  }
}